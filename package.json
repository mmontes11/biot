{
  "name": "biot",
  "version": "1.0.1",
  "description": "Telegram IoT bot that provides info about measurements and events",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon  src/index.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "build": "babel -d dist/ src/",
    "dist": "npm run build && cp package.json dist/ && cd dist && npm install --production && cd -",
    "production": "cross-env NODE_ENV=production node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmontes11/biot.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "iot",
    "nodejs"
  ],
  "author": "Mart√≠n Montes <martin11lrx@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmontes11/biot/issues"
  },
  "homepage": "https://github.com/mmontes11/biot#readme",
  "dependencies": {
    "@mmontes11/iot-client": "^1.0.0",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "express-basic-auth": "^1.1.3",
    "express-winston": "^2.4.0",
    "helmet": "^3.10.0",
    "http-status": "^1.0.1",
    "method-override": "^2.3.10",
    "node-emoji": "^1.8.1",
    "node-telegram-bot-api": "^0.30.0",
    "underscore": "^1.8.3",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "nodemon": "^1.14.8"
  }
}
