{
  "name": "biot",
  "version": "0.0.1",
  "description": "Telegram IoT bot that provides info about measurements and events",
  "main": "src/index.js",
  "scripts": {
    "start": "nodemon src/index.js --exec babel-node",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/*",
    "prebuild": "npm run clean",
    "build": "babel -d dist/ src/",
    "dist": "npm run build && cp package.json dist/ && cd dist && npm install --production && cd -",
    "production": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmontes11/biot.git"
  },
  "keywords": [
    "telegram",
    "bot",
    "iot",
    "nodejs"
  ],
  "author": "Mart√≠n Montes <martin11lrx@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mmontes11/biot/issues"
  },
  "homepage": "https://github.com/mmontes11/biot#readme",
  "dependencies": {
    "@mmontes11/iot-client": "latest",
    "http-status": "^1.0.1",
    "node-telegram-bot-api": "^0.30.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "nodemon": "^1.14.8"
  }
}
